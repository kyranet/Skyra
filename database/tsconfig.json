{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": ".",
    "composite": true,
	"sourceMap": false,
	"esModuleInterop": true,
	"module": "CommonJS",
    "paths": {
      "@utils/*": ["../src/lib/util/*"],
      "@lib/*": ["../src/lib/*"],
      "@root/*": ["../src/*"],
      "@dist/*": ["../dist/*"]
    }
  },
  "include": ["*.ts"],
  "references": [{ "path": "../src" }]
}
