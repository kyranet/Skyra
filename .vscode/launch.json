{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debugger",
			"args": ["${workspaceFolder}/dist/Skyra.js"],
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"NODE_ENV": "development"
			},
			"skipFiles": ["<node_internals>/**", "node_modules/tslib/**"],
			"console": "internalConsole",
			"outputCapture": "std",
			"outFiles": ["${workspaceFolder}/dist/**/*.js"]
		}
	]
}
