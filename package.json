{
  "name": "skyra",
  "version": "3.4.0",
  "description": "Standalone Discord Bot built on Klasa",
  "main": "src/skyra.js",
  "scripts": {
    "pm2:delete": "pm2 delete Skyra",
    "pm2:logs": "pm2 logs Skyra --lines=200",
    "pm2:restart": "pm2 restart Skyra",
    "pm2:start": "pm2 start src/skyra.js --name=\"Skyra\"",
    "pm2:start-dev": "pm2 start src/skyra.js --name=\"Skyra\" --env={ \"DEV\": \"true\" }",
    "pm2:start-harmony": "pm2 start src/skyra.js --node-args=\"--harmony\" --name=\"Skyra\"",
    "pm2:start-harmony-dev": "pm2 start src/skyra.js --node-args=\"--harmony\" --name=\"Skyra\" --env={ \"DEV\": \"true\" }",
    "pm2:stop": "pm2 stop Skyra",
    "dev:unix": "DEV=true node src/skyra.js",
    "dev:windows": "$env:DEV=\"true\"; node src/skyra.js",
    "test": "tslint src/**/*.ts",
    "lint": "tslint --fix src/**/*.ts",
    "deps:update": "yarn install dirigeants/klasa discordjs/discord.js",
    "deps:install": "yarn install --production"
  },
  "keywords": [
    "discord.js",
    "klasa",
    "bot",
    "standalone"
  ],
  "author": "kyranet",
  "license": "UNLICENSED",
  "dependencies": {
    "canvas": "github:Automattic/node-canvas#92b192447e9b9ae98da0f801e4e34afdd1dc1ef8",
    "canvas-constructor": "^1.1.2",
    "diff": "^3.5.0",
    "discord.js": "github:discordjs/discord.js",
    "fs-nextra": "^0.3.7",
    "gifencoder": "^1.1.0",
    "heapdump": "^0.3.9",
    "klasa": "github:dirigeants/klasa",
    "node-fetch": "^2.2.0",
    "rethinkdb-ts": "^2.4.0-rc.7",
    "veza": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^10.11.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyranet/Skyra.git"
  },
  "bugs": {
    "url": "https://github.com/kyranet/Skyra/issues"
  },
  "homepage": "https://github.com/kyranet/Skyra#readme"
}
